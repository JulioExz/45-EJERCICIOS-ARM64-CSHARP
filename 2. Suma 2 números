REFERENCIA 
using System;

class Program {
    static void Main() {
        int a = 5, b = 3;
        int sum = a + b;
        Console.WriteLine("Suma: " + sum);
    }
}
--------------------------------------------------------------------------------
    .section .data
msg_sum:
    .asciz "Suma: %d\n"

    .section .text
    .global _start

_start:
    mov x0, 5                  // Primer número
    mov x1, 3                  // Segundo número
    add x2, x0, x1             // Suma x0 y x1 en x2

    // Imprimir resultado
    mov x0, 1
    ldr x1, =msg_sum
    mov x8, 64
    svc 0

    mov x0, 0
    mov x8, 93
    svc 0


