# ------------------------------------------------------------------------------
# Nombre del Programa: Muestra el resultado factorial de un numero
# Descripción      : Muestra el resultado factorial de un numero
# Autor            : [De Haro Exzacarias Julio Cesar]
# Fecha            : [05-11-2024]
# Versión          : 1.0
# Arquitectura     : ARM64 en AWS EC2
# ------------------------------------------------------------------------------
REFERENCIA
using System;

class Program {
    static void Main() {
        int n = 5;
        int factorial = 1;
        for (int i = 1; i <= n; i++) {
            factorial *= i;
        }
        Console.WriteLine("Factorial: " + factorial);
    }
}
--------------------------------------------------------------------------------------
    .section .data
msg_fact:
    .asciz "Factorial: %d\n"

    .section .text
    .global _start

_start:
    mov x0, 5                  // Valor de n
    mov x2, 1                  // Inicializar factorial en 1
factorial_loop:
    cmp x0, 1                  // Comparar n con 1
    ble end_factorial          // Si n <= 1, fin del bucle
    mul x2, x2, x0             // factorial *= n
    sub x0, x0, 1              // n--
    b factorial_loop           // Repetir el bucle

end_factorial:
    // Imprimir resultado
    mov x0, 1
    ldr x1, =msg_fact
    mov x8, 64
    svc 0

    mov x0, 0
    mov x8, 93
    svc 0
