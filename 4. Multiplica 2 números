REFERENCIA
using System;

class Program {
    static void Main() {
        int a = 5, b = 3;
        int product = a * b;
        Console.WriteLine("Producto: " + product);
    }
}
---------------------------------------------------------------------------------
    .section .data
msg_product:
    .asciz "Producto: %d\n"

    .section .text
    .global _start

_start:
    mov x0, 5                  // Primer número
    mov x1, 3                  // Segundo número
    mul x2, x0, x1             // Multiplica x0 * x1 en x2

    // Imprimir resultado
    mov x0, 1
    ldr x1, =msg_product
    mov x8, 64
    svc 0

    mov x0, 0
    mov x8, 93
    svc 0
