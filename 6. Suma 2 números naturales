REFERENCIA
using System;

class Program {
    static void Main() {
        int N = 10;
        int sum = N * (N + 1) / 2;
        Console.WriteLine("Suma: " + sum);
    }
}
--------------------------------------------------------------------------------
    .section .data
msg_sum_n:
    .asciz "Suma de N: %d\n"

    .section .text
    .global _start

_start:
    mov x0, 10                 // Valor de N
    add x1, x0, 1              // N + 1
    mul x2, x0, x1             // N * (N + 1)
    mov x3, 2
    udiv x2, x2, x3            // (N * (N + 1)) / 2 en x2

    // Imprimir resultado
    mov x0, 1
    ldr x1, =msg_sum_n
    mov x8, 64
    svc 0

    mov x0, 0
    mov x8, 93
    svc 0
