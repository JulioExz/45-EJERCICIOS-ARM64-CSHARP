REFERENCIA
using System;

class Program {
    static void Main() {
        int a = 10, b = 2;
        int quotient = a / b;
        Console.WriteLine("División: " + quotient);
    }
}
---------------------------------------------------------------------------------
    .section .data
msg_quotient:
    .asciz "División: %d\n"

    .section .text
    .global _start

_start:
    mov x0, 10                 // Dividendo
    mov x1, 2                  // Divisor
    udiv x2, x0, x1            // División x0 / x1 en x2

    // Imprimir resultado
    mov x0, 1
    ldr x1, =msg_quotient
    mov x8, 64
    svc 0

    mov x0, 0
    mov x8, 93
    svc 0


